---

<div class="title entity single">
  <h2><%= title %></h2>
</div>

<div class="sharing-right">
  <%= partial('partials/sharing_buttons') %>
</div>

<div class="row">

  <div class="entity-info col-sm-3">
    <div class="panel panel-default">
      <%= image_tag( lorem.image('300x300'), class: 'img-responsive') %>
      <div class="panel-body">
        <p class="entity-title"><%= title %></p>
        <hr class="title-lines-horizontal">
        <p class="description"><%= lorem.sentence %></p>
        <hr class="title-lines-horizontal">
        <p class="entity-data"><strong><%= lorem.words 2 %></strong> <span class="pull-right"><%= rand(100000) %></span></p>
        <p class="entity-data"><strong><%= lorem.words 2 %></strong> <span class="pull-right"><%= rand(100000) %></span></p>
        <p class="entity-data"><strong><%= lorem.words 2 %></strong> <span class="pull-right"><%= rand(100000) %></span></p>
      </div>
      <div class='entity-links'>
        <ul></ul>
      </div>
    </div>
  </div>

  <div class="col-sm-9">
    <div class="panel panel-default">
      <%= image_tag( lorem.image('900x500'), class: 'img-responsive') %>
    </div>
  </div>


  <div class="col-sm-12">
    <div class="panel panel-default">
      <%= partial('partials/contracts_table', :locals => { :company => title }) %>
    </div>
  </div>

  <div class="col-sm-12">
    <div class="panel panel-default">
      <iframe src="http://quienmanda.es/entities/ferrovial?widget=1&history=" width="100%" height="450px" scrolling="no" marginheight="0" frameborder="0"></iframe>
    </div>
  </div>

</div>
